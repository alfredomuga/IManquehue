{
	"name": "ThHitosActAut",
	"properties": {
		"folder": {
			"name": "Planificación"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Real",
						"type": "DatasetReference"
					},
					"name": "Real"
				},
				{
					"dataset": {
						"referenceName": "DimProyectoEtapa",
						"type": "DatasetReference"
					},
					"name": "DimProyectoEtapa"
				},
				{
					"dataset": {
						"referenceName": "DimEscenario",
						"type": "DatasetReference"
					},
					"name": "DimEscenario"
				},
				{
					"dataset": {
						"referenceName": "DimHitos",
						"type": "DatasetReference"
					},
					"name": "DimHitos"
				},
				{
					"dataset": {
						"referenceName": "dboFecha",
						"type": "DatasetReference"
					},
					"name": "dbfecha"
				},
				{
					"dataset": {
						"referenceName": "Forecast",
						"type": "DatasetReference"
					},
					"name": "Forescast"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ThHitosReal2",
						"type": "DatasetReference"
					},
					"name": "ThHitosReal"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "NoNulos"
				},
				{
					"name": "idEtapaProyecto"
				},
				{
					"name": "idHitos"
				},
				{
					"name": "idEscenario"
				},
				{
					"name": "AgrEscenario"
				},
				{
					"name": "idfecha"
				},
				{
					"name": "alterRow1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "select2"
				},
				{
					"name": "filter1"
				},
				{
					"name": "idProyectoEtapaFor"
				},
				{
					"name": "idHitosFor"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Proyecto as string,",
				"          Etapa as string,",
				"          Hito as string,",
				"          Mes as string,",
				"          {Año} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Real",
				"source(output(",
				"          idProyectoEtapa as integer,",
				"          Proyecto as string,",
				"          EtapaComercial as string,",
				"          EtapaFinanciera as string,",
				"          Unidades as integer,",
				"          RutEmpresa as string,",
				"          Segmento as string,",
				"          ParticipacionSocios as double,",
				"          Socio as string,",
				"          Comuna as string,",
				"          Constructora as string,",
				"          CostoTotal as double,",
				"          ValorTerreno as double,",
				"          LineaConstruccion as double,",
				"          ProximoHito as string,",
				"          FechaBudget as date,",
				"          FechaForecast as date,",
				"          Variacion as double,",
				"          EstadoProyecto as string,",
				"          TipoFinanciamientoSocios as string,",
				"          VigenciaDeuda as string,",
				"          TipoActivo as string,",
				"          EtapaGestion as string,",
				"          AtrasoMeses as integer,",
				"          EtapaConstructora as string,",
				"          Codigo as string,",
				"          Considerar as string,",
				"          Empresa as string,",
				"          BancoAlzante as string,",
				"          IVA as double,",
				"          IFRS as double,",
				"          Consolida as string,",
				"          Act as integer,",
				"          Act_Comercial as integer,",
				"          Act_Masterfechas as string,",
				"          Act_FlujoCaja as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> DimProyectoEtapa",
				"source(output(",
				"          idEscenario as string,",
				"          Escenario as string,",
				"          Codigo as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> DimEscenario",
				"source(output(",
				"          idHito as string,",
				"          Hito as string,",
				"          Codigo as string,",
				"          Orden as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> DimHitos",
				"source(output(",
				"          idFecha as string,",
				"          Anho as string,",
				"          NumeroMesCero as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> dbfecha",
				"source(output(",
				"          Periodo as string,",
				"          Proyecto as string,",
				"          Etapa as string,",
				"          Hito as string,",
				"          Mes as string,",
				"          {Año} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Forescast",
				"Real select(mapColumn(",
				"          Proyecto,",
				"          Etapa,",
				"          Hito,",
				"          Mes,",
				"          {Año}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 filter(Hito!=\"\"||Mes!=\"\"||{Año}!=\"\") ~> NoNulos",
				"AgrEscenario, DimProyectoEtapa lookup(select1@Proyecto == DimProyectoEtapa@Proyecto",
				"     && Etapa == EtapaGestion,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> idEtapaProyecto",
				"idEtapaProyecto, DimHitos lookup(select1@Hito == DimHitos@Hito,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> idHitos",
				"idHitos, DimEscenario lookup(AgrEscenario@Escenario == DimEscenario@Escenario,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> idEscenario",
				"NoNulos derive(Escenario = '0',",
				"          anho = {Año},",
				"          mes = toString(toInteger(Mes)*10)) ~> AgrEscenario",
				"idEscenario, dbfecha lookup(AgrEscenario@anho == dbfecha@Anho",
				"     && AgrEscenario@mes == NumeroMesCero,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> idfecha",
				"derivedColumn1 alterRow(insertIf(true())) ~> alterRow1",
				"idfecha derive(idFecha = toInteger(idFecha),",
				"          idEscenario = toInteger(idEscenario)) ~> derivedColumn1",
				"Forescast select(mapColumn(",
				"          Periodo,",
				"          Proyecto,",
				"          Etapa,",
				"          Hito,",
				"          Mes,",
				"          {Año}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"select2 filter(Hito!=\"\"||Mes!=\"\"||{Año}!=\"\") ~> filter1",
				"filter1, DimProyectoEtapa lookup(select2@Proyecto == DimProyectoEtapa@Proyecto",
				"     && Etapa == EtapaGestion,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> idProyectoEtapaFor",
				"idProyectoEtapaFor, DimHitos lookup(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> idHitosFor",
				"alterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          idFecha as integer,",
				"          idProyectoEtapa as integer,",
				"          idEscenario as integer,",
				"          CodigoHito as string,",
				"          OrdenHito as integer,",
				"          Proyecto as string,",
				"          Etapa as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     preSQLs:['truncate table th.HitosReal'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          idFecha,",
				"          idProyectoEtapa,",
				"          idEscenario,",
				"          CodigoHito = DimHitos@Codigo,",
				"          OrdenHito = Orden,",
				"          Proyecto = Etapa",
				"     )) ~> ThHitosReal"
			]
		}
	}
}